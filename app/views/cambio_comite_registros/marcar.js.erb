<% if @cambio_comite_registro.revisado == true %>
    $(".cambio-comite-registro-<%= @cambio_comite_registro.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".cambio-comite-registro-<%= @cambio_comite_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
