<p id="notice"><%= notice %></p>

<h1>Consejeros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consejeros.each do |consejero| %>
      <tr>
        <td><%= consejero.nombre %></td>
        <td><%= link_to 'Show', consejero %></td>
        <td><%= link_to 'Edit', edit_consejero_path(consejero) %></td>
        <td><%= link_to 'Destroy', consejero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consejero', new_consejero_path %>
