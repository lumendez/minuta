<%= form_for(user) do |f| %>
  <% if user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nombre %>
    <%= f.text_field :nombre %>
  </div>

  <div class="field">
    <%= f.label :boleta %>
    <%= f.text_field :boleta %>
  </div>

  <div class="field">
    <%= f.label :sepi_programa_id %>
    <%= collection_select(:user, :sepi_programa_id, SepiPrograma.all, :id, :nombre, {prompt: true}) %>
  </div>

  <div class="field">
    <%= f.label :consejero_id %>
    <%= collection_select(:user, :consejero_id, Consejero.all, :id, :nombre, {prompt: true}) %>
  </div>

  <div class="field">
    <%= f.label :coordinador_nombre_id %>
    <%= collection_select(:user, :coordinador_nombre_id, CoordinadorNombre.all, :id, :nombre, {prompt: true}) %>
  </div>

  <div class="field">
    <%= f.label :tipos_usuario_id %>
    <%= collection_select(:user, :tipos_usuario_id, TiposUsuario.all, :id, :tipo, {prompt: true}) %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
