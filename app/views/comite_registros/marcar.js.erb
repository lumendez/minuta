<% if @comite_registro.revisado == true %>
    $(".comite-registro-<%= @comite_registro.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".comite-registro-<%= @comite_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
