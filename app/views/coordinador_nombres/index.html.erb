<p id="notice"><%= notice %></p>

<h1>Coordinador Nombres</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordinador_nombres.each do |coordinador_nombre| %>
      <tr>
        <td><%= coordinador_nombre.nombre %></td>
        <td><%= link_to 'Show', coordinador_nombre %></td>
        <td><%= link_to 'Edit', edit_coordinador_nombre_path(coordinador_nombre) %></td>
        <td><%= link_to 'Destroy', coordinador_nombre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coordinador Nombre', new_coordinador_nombre_path %>
