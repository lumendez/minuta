<% if @tema_tesis_registro.valida_coordinador == true %>
    $(".tema-tesis-registro-<%= @tema_tesis_registro.id %> > a").html('Validado').removeClass('btn-danger').addClass('btn-success')
<% else %>
    $(".tema-tesis-registro-<%= @tema_tesis_registro.id %> > a").html('No validado').removeClass('btn-success').addClass('btn-danger')
<% end %>
