<% if @comision_registro.valida_coordinador == true %>
    $(".comision-registro-<%= @comision_registro.id %> > a").html('Validado').removeClass('btn-danger').addClass('btn-success')
<% else %>
    $(".comision-registro-<%= @comision_registro.id %> > a").html('No validado').removeClass('btn-success').addClass('btn-danger')
<% end %>
