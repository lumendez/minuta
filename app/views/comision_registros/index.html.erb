<p id="notice"><%= notice %></p>

<h1>Comision Registros</h1>

<table>
  <thead>
    <tr>
      <th>Presidente</th>
      <th>Secretario</th>
      <th>Primer vocal</th>
      <th>Segundo vocal</th>
      <th>Tercer vocal</th>
      <th>Suplente</th>
      <th>Valida consejero</th>
      <th>Valida coordinador</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comision_registros.each do |comision_registro| %>
      <tr>
        <td><%= comision_registro.presidente %></td>
        <td><%= comision_registro.secretario %></td>
        <td><%= comision_registro.primer_vocal %></td>
        <td><%= comision_registro.segundo_vocal %></td>
        <td><%= comision_registro.tercer_vocal %></td>
        <td><%= comision_registro.suplente %></td>
        <td><%= comision_registro.valida_consejero %></td>
        <td><%= comision_registro.valida_coordinador %></td>
        <td><%= comision_registro.estado %></td>
        <td><%= link_to 'Show', comision_registro %></td>
        <td><%= link_to 'Edit', edit_comision_registro_path(comision_registro) %></td>
        <td><%= link_to 'Destroy', comision_registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comision Registro', new_comision_registro_path %>
