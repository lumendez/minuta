<% if @comision_registro.revisado == true %>
    $(".comision-registro-<%= @comision_registro.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".comision-registro-<%= @comision_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
