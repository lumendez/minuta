<p id="notice"><%= notice %></p>

<h1>Electrica Consejeros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @electrica_consejeros.each do |electrica_consejero| %>
      <tr>
        <td><%= electrica_consejero.nombre %></td>
        <td><%= link_to 'Show', electrica_consejero %></td>
        <td><%= link_to 'Edit', edit_electrica_consejero_path(electrica_consejero) %></td>
        <td><%= link_to 'Destroy', electrica_consejero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

