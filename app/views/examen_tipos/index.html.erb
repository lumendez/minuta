<p id="notice"><%= notice %></p>

<h1>Examen Tipos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examen_tipos.each do |examen_tipo| %>
      <tr>
        <td><%= examen_tipo.tipo %></td>
        <td><%= link_to 'Show', examen_tipo %></td>
        <td><%= link_to 'Edit', edit_examen_tipo_path(examen_tipo) %></td>
        <td><%= link_to 'Destroy', examen_tipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Examen Tipo', new_examen_tipo_path %>
