<% if @director_tesis_registro.revisado == true %>
    $(".director-tesis-registro-<%= @director_tesis_registro.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".director-tesis-registro-<%= @director_tesis_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
