<% if @director_tesis_registro.valida_consejero == true %>
    $(".director-tesis-registro-<%= @director_tesis_registro.id %> > a").html('Validado').removeClass('btn-danger').addClass('btn-success')
<% else %>
    $(".director-tesis-registro-<%= @director_tesis_registro.id %> > a").html('No validado').removeClass('btn-success').addClass('btn-danger')
<% end %>
