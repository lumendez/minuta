<p id="notice"><%= notice %></p>

<h1>Consejero Electricas</h1>

<table class="table table-bordered">

  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <% @consejero_electricas.each do |consejero_electrica| %>
      <tr>
        <td><%= consejero_electrica.nombre %></td>
        <td><%= link_to 'Detalles'
, consejero_electrica %></td>
        <td><%= link_to 'Editar', edit_consejero_electrica_path(consejero_electrica) %></td>
        <td><%= link_to 'Eliminar', consejero_electrica, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consejero Electrica', new_consejero_electrica_path %>
