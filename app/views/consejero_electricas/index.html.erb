<p id="notice"><%= notice %></p>

<h1>Consejero Electricas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consejero_electricas.each do |consejero_electrica| %>
      <tr>
        <td><%= consejero_electrica.nombre %></td>
        <td><%= link_to 'Show', consejero_electrica %></td>
        <td><%= link_to 'Edit', edit_consejero_electrica_path(consejero_electrica) %></td>
        <td><%= link_to 'Destroy', consejero_electrica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

