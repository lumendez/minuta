<% if @cambio_comision_registro.revisado == true %>
    $(".cambio-comision-registro-<%= @cambio_comision_registro.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".cambio-comision-registro-<%= @cambio_comision_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
