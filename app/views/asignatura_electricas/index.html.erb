<p id="notice"><%= notice %></p>

<h1>Asignatura Electricas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asignatura_electricas.each do |asignatura_electrica| %>
      <tr>
        <td><%= asignatura_electrica.nombre %></td>
        <td><%= link_to 'Show', asignatura_electrica %></td>
        <td><%= link_to 'Edit', edit_asignatura_electrica_path(asignatura_electrica) %></td>
        <td><%= link_to 'Destroy', asignatura_electrica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asignatura Electrica', new_asignatura_electrica_path %>
