<p id="notice"><%= notice %></p>

<h1>Asignatura Electricas</h1>

<table class="table table-bordered">

  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3">Opciones</th>
    </tr>
  </thead>

  <tbody>
    <% @asignatura_electricas.each do |asignatura_electrica| %>
      <tr>
        <td><%= asignatura_electrica.nombre %></td>
        <td><%= link_to 'Detalles'
, asignatura_electrica %></td>
        <td><%= link_to 'Editar', edit_asignatura_electrica_path(asignatura_electrica) %></td>
        <td><%= link_to 'Eliminar', asignatura_electrica, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asignatura Electrica', new_asignatura_electrica_path %>
