<% if @cambiar_tema.revisado == true %>
    $(".cambiar-tema-<%= @cambiar_tema.id %> > a").html('Revisado').removeClass('btn-warning').addClass('btn-success')
<% else %>
    $(".cambiar-tema-<%= @cambiar_tema.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-warning')
<% end %>
