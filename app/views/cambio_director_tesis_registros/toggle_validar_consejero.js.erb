<% if @cambio_director_tesis_registro.valida_consejero == true %>
    $(".cambio-director-tesis-registro-<%= @cambio_director_tesis_registro.id %> > a").html('Validado').removeClass('btn-danger').addClass('btn-success')
<% else %>
    $(".cambio-director-tesis-registro-<%= @cambio_director_tesis_registro.id %> > a").html('No revisado').removeClass('btn-success').addClass('btn-danger')
<% end %>
