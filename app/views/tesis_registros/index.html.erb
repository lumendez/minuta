<p id="notice"><%= notice %></p>

<h1>Tesis Registros</h1>

<table>
  <thead>
    <tr>
      <th>Tema</th>
      <th>Director</th>
      <th>Presidente</th>
      <th>Secretario</th>
      <th>Primer vocal</th>
      <th>Segundo vocal</th>
      <th>Tercer vocal</th>
      <th>Suplente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tesis_registros.each do |tesis_registro| %>
      <tr>
        <td><%= tesis_registro.tema %></td>
        <td><%= tesis_registro.director %></td>
        <td><%= tesis_registro.presidente %></td>
        <td><%= tesis_registro.secretario %></td>
        <td><%= tesis_registro.primer_vocal %></td>
        <td><%= tesis_registro.segundo_vocal %></td>
        <td><%= tesis_registro.tercer_vocal %></td>
        <td><%= tesis_registro.suplente %></td>
        <td><%= link_to 'Show', tesis_registro %></td>
        <td><%= link_to 'Edit', edit_tesis_registro_path(tesis_registro) %></td>
        <td><%= link_to 'Destroy', tesis_registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tesis Registro', new_tesis_registro_path %>
