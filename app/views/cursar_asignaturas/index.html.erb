<p id="notice"><%= notice %></p>

<h1>Cursar Asignaturas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Clave</th>
      <th>Unidad</th>
      <th>Creado por</th>
      <th>Boleta</th>
      <th>Fecha</th>
      <th>Validacion consejero</th>
      <th>Validacion coordinador</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cursar_asignaturas.each do |cursar_asignatura| %>
      <tr>
        <td><%= cursar_asignatura.nombre %></td>
        <td><%= cursar_asignatura.clave %></td>
        <td><%= cursar_asignatura.unidad %></td>
        <td><%= cursar_asignatura.user.nombre %></td>
        <td><%= cursar_asignatura.user.boleta %></td>
        <td><%= cursar_asignatura.created_at.strftime("%d/%m/%y a las %T %P") %></td>
        <td><%= cursar_asignatura.valida_consejero ? 'Si' : 'No' %></td>
        <td><%= cursar_asignatura.valida_coordinador ? 'Si' : 'No' %></td>
        <td><%= cursar_asignatura.estado %></td>
        <td><%= link_to 'Show', cursar_asignatura %></td>
        <td><%= link_to 'Edit', edit_cursar_asignatura_path(cursar_asignatura) %></td>
        <td><%= link_to 'Destroy', cursar_asignatura, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cursar Asignatura', new_cursar_asignatura_path %>
