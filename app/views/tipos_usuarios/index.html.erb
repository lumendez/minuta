<p id="notice"><%= notice %></p>

<h1>Tipos Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_usuarios.each do |tipos_usuario| %>
      <tr>
        <td><%= tipos_usuario.tipo %></td>
        <td><%= link_to 'Show', tipos_usuario %></td>
        <td><%= link_to 'Edit', edit_tipos_usuario_path(tipos_usuario) %></td>
        <td><%= link_to 'Destroy', tipos_usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipos Usuario', new_tipos_usuario_path %>
